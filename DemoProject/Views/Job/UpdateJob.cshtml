@model EntityLayer.Concrete.Job
@{
    ViewData["Title"] = "UpdateJob";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Meslek Düzenleme Kaydı</h4>
            <form class="forms-sample" method="post">

                <div class="form-group">
                    @Html.HiddenFor(X=>X.JobId)
                    <label for="exampleInputName">Meslek Adı</label>
                    <input type="text" class="form-control" asp-for="JobName">
                </div>
               

                <button type="submit" class="btn btn-outline-success me-2">Düzenle</button>
            </form>
        </div>
    </div>
</div>

